<div class="ControlsDemo-CompoundControl">
   <h3>Compound test control</h3>
   <div if="{{ someOption }}" class="ControlsDemo-CompoundControl__someOption">Value: {{ someOption }}</div>
   <div if="{{ newOption }}" class="ControlsDemo-CompoundControl__newOption">Got new option</div>
   <ws:if data="{{ buildChild }}">
      <UIDemo.Compound.CompoundChild name="innerNestedChild" />
   </ws:if>
</div>
